= simple_form_for(@company_contact) do |f|
  = f.error_notification

  .form-inputs
    .form-group
      .span1
        = f.label 'Телефон'
      = f.input_field :phone
    .form-group
      .span1
        = f.label 'Адрес'
      = f.input_field :adress
    .form-group
      .span1
        = f.label 'ФИО контактного лица'
      = f.input_field :fio

  .form-actions
    = f.button :submit, 'Сохранить'
